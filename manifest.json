{
  "manifest_version": 2,
  "name": "Fast Jira",
  "version": "1.0.0",
  "description": "Quickly opens the JIRA with the corresponding ticket",
  "icons": {"128": "Icon_128.png"},
  "browser_action": {
          "default_icon": "Icon.png",
		  "default_popup": "popup.html"
		 },
	"options_ui": {
    "page": "options.html",
    "chrome_style": true
	
},

	"permissions": ["tabs",
	"storage"]		
}